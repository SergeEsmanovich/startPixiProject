  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outFile": "Main.js",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "Main.ts"
  ],
  "compileOnSave": true
}